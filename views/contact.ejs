                <script>
                    // No additional link required, SweetAlert2 is already included above
                    const scriptUrl = 'https://script.google.com/macros/s/AKfycbxLDX7ywFwz5TXAzqYGjESEy0m3Vx4tIWEuUIqHYfpk3ySdw_XGQi0i0YgiBAVrK0DsvQ/exec';
                    function validateMobile(mobile) {
                        return /^[0-9]{7,15}$/.test(mobile);
                    }

                    document.addEventListener('DOMContentLoaded', function () {
                        const form = document.getElementById('contact-form');
                        const statusDiv = document.getElementById('form-status');
                        const submitBtn = document.getElementById('submit-btn');
                        const mobileInput = document.getElementById('mobile');
                        const mobileInvalid = document.getElementById('mobile-invalid-feedback');

                        mobileInput.addEventListener('input', function () {
                            if (validateMobile(mobileInput.value)) {
                                mobileInput.classList.remove('is-invalid');
                                mobileInvalid.style.display = 'none';
                            } else {
                                mobileInput.classList.add('is-invalid');
                                mobileInvalid.style.display = 'block';
                            }
                        });

                        form.addEventListener('submit', function (e) {
                            e.preventDefault();
                            statusDiv.innerHTML = '';

                            // Validate mobile before submit
                            if (!validateMobile(mobileInput.value)) {
                                mobileInput.classList.add('is-invalid');
                                mobileInvalid.style.display = 'block';
                                mobileInput.focus();
                                return;
                            }

                            submitBtn.disabled = true;
                            submitBtn.innerHTML = 'Sending...';

                            // Create URL-encoded form data
                            const formData = new URLSearchParams();
                            formData.append('name', document.getElementById('name').value);
                            formData.append('email', document.getElementById('email').value);
                            formData.append('mobile', document.getElementById('mobile').value);
                            formData.append('subject', document.getElementById('subject').value);
                            formData.append('message', document.getElementById('comments').value);

                            fetch(scriptUrl, {
                                method: 'POST',
                                headers: {
                                    'Content-Type': 'application/x-www-form-urlencoded',
                                },
                                body: formData
                            })
                                .then(response => {
                                    if (!response.ok) {
                                        throw new Error('Network response was not ok');
                                    }
                                    return response.json();
                                })
                                .then(data => {
                                    if (data.status === "success") {
                                        form.reset();
                                        // SweetAlert2 thank you and content
                                        Swal.fire({
                                            icon: 'success',
                                            title: 'Thank You for Reaching Out!',
                                            html: `
