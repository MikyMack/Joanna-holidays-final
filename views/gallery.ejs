                                    <iframe
                                        src="https://www.youtube.com/embed/<%= (video.youtubeLink.match(/(?:youtu\.be\/|youtube\.com\/(?:embed\/|v\/|watch\?v=|watch\?.+&v=))([^&\n?#]+)/) || [])[1] %>"
                                        title="<%= video.title || 'YouTube Video' %>" frameborder="0"
                                        allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
                                        allowfullscreen style="width:100%;height:100%;min-height:200px;"></iframe>
                                </div>
                                <% if (video.title) { %>
                                    <div class="mt-2 text-center text-secondary fw-medium">
                                        <%= video.title %>
                                    </div>
                                <% } %>
                            </div>
                        <% }) %>
