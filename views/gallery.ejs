    <section class="gallery-section py-5" style="background: #f8fafc;">
        <div class="container">
            <!-- Tab Buttons -->
            <div class="row justify-content-center mb-4">
                <div class="col-auto">
                    <button id="imagesTabBtn" type="button" class="btn btn-danger me-2"
                        onclick="showGalleryTab('images')">Images</button>
                    <button id="videosTabBtn" type="button" class="btn btn-outline-secondary"
                        onclick="showGalleryTab('videos')">Videos</button>
                </div>
            </div>

            <!-- Images Tab -->
            <div id="imagesTab" class="tab-pane">
                <div class="row g-3 g-md-4">
                    <% if (gallery && gallery.length> 0) { %>
                        <% gallery.forEach(function(item, idx) { %>
                            <div class="col-6 col-md-4">
                                <div class="gallery-img-card rounded shadow-sm overflow-hidden position-relative"
                                    style="background: #fff;">
                                    <a class="d-block gallery-lightbox-trigger" href="javascript:void(0);"
                                        data-img="<%= item.imageUrl %>" data-title="<%= item.title || '' %>"
                                        aria-label="Open image preview">
                                        <img src="<%= item.imageUrl %>"
                                            class="img-fluid w-100 h-100 object-fit-cover gallery-img"
                                            alt="<%= item.title || 'Gallery Image' %>" loading="lazy"
                                            style="aspect-ratio: 1/1; object-fit: cover;">
                                    </a>
                                </div>
                            </div>
                            <% }) %>
                                <% } else { %>
                                    <div class="col-12 text-center text-muted py-5">No images found.</div>
                                    <% } %>
                </div>
                <!-- Pagination -->
                <div class="d-flex justify-content-center mt-4 gap-2">
                    <% if (currentPage> 1) { %>
                        <a href="?page=<%= currentPage - 1 %>&limit=12" class="btn btn-outline-dark">Previous</a>
                        <% } %>
                            <% for (let i=1; i <=totalPages; i++) { %>
                                <a href="?page=<%= i %>&limit=12"
                                    class="btn <%= currentPage === i ? 'btn-danger text-white' : 'btn-outline-dark' %>">
                                    <%= i %>
                                </a>
                                <% } %>
                                    <% if (currentPage < totalPages) { %>
                                        <a href="?page=<%= currentPage + 1 %>&limit=12"
                                            class="btn btn-outline-dark">Next</a>
                                        <% } %>
                </div>
            </div>

            <!-- Videos Tab -->
