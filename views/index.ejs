<body>

    <!-- Google Tag Manager (noscript) -->
<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-KV49GR89"
    height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) -->
    <!-- Preloader -->
    <div class="preloader" id="preloader">
        <div class="spinner-grow" role="status">
            <span class="visually-hidden">Loading...</span>
        </div>
    </div>

    <!-- Search Form Overlay -->
    <div class="search-bg-overlay" id="searchOverlay"></div>

    <!-- Search Form Popup -->
    <div class="search-form-popup">
        <h2 class="mb-5 display-6 fw-bold text-white">How can I help you, Today?</h2>
        <button type="button" class="close-btn" id="searchClose" aria-label="Close">
            <i class="ti ti-x"></i>
        </button>
        <form class="search-form" id="searchForm">
            <input type="search" name="search" class="form-control" placeholder="Search..." required minlength="3">
            <button type="submit" class="btn btn-success d-none"><i class="ti ti-search"></i></button>
          </form>
          <script>
            document.getElementById('searchForm').addEventListener('submit', function (e) {
              e.preventDefault();
              const input = this.querySelector('input[name="search"]');
              const query = input.value.trim();
          
              if (query.length >= 3) {
                // Redirect to /packages with search query
                const encoded = encodeURIComponent(query);
                window.location.href = `/packages?search=${encoded}`;
              } else {
                alert('Please enter at least 3 characters to search.');
              }
            });
          </script>
    </div>

    <!-- Header Area-->
    <header class="header-area style-three">
        <div class="container-fluid">
            <nav class="navbar navbar-expand-xl">
                <!-- Navbar Brand -->
                <a class="navbar-brand" href="/">
                    <img src="/img/joanna/joannalogo.png" alt="joanna" class="joannalogo">
                </a>

                <!-- Navbar Toggler -->
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#touriaNav"
                    aria-controls="touriaNav" aria-expanded="false" aria-label="Toggle navigation">
                    <i class="ti ti-menu-deep"></i>
                </button>

                <!-- Navbar Nav -->
                <div class="collapse justify-content-xl-end navbar-collapse" id="touriaNav">
                    <ul class="navbar-nav align-items-xl-center navbar-nav-scroll">
                        <!-- Home -->
                        <li class="touria-dd">
                            <a href="/" style="color: #334ea2;">Home </a>
                        </li>
                        <!-- Packages (Dynamic) -->
                        <li class="touria-dd">
                            <a href="/packages">Packages <i class="ti ti-chevron-down"></i></a>
                            <ul class="touria-dd-menu">
                                <% categories.forEach(category=> {
                                    const hasSubCategories = category.subCategories && category.subCategories.length >
                                    0;
                                    const hasDirectPackages = category.directPackages && category.directPackages.length
                                    > 0;
                                    %>
                                    <% if (hasSubCategories || hasDirectPackages) { %>
                                        <li class="touria-dd">
                                            <a href="/packages?category=<%= category._id %>">
                                                <%= category.name %>
                                                    <% if (hasSubCategories || hasDirectPackages) { %>
                                                        <i class="ti ti-chevron-right"></i>
                                                        <% } %>
                                            </a>
                                            <ul class="touria-dd-menu">
                                                <% category.subCategories &&
                                                    category.subCategories.forEach(subCategory=> { %>
                                                    <li>
                                                        <a href="/package-details?subCategory=<%= subCategory._id %>">
                                                            <%= subCategory.name %>
                                                        </a>
                                                    </li>
                                                    <% }) %>
                                                        <% category.directPackages &&
                                                            category.directPackages.forEach(pkg=> { %>
                                                            <li>
                                                                <a href="/package/<%= pkg._id %>">
                                                                    <%= pkg.destination %>
                                                                </a>
                                                            </li>
                                                            <% }) %>
                                            </ul>
                                        </li>
                                        <% } else { %>
                                            <li>
                                                <a href="/packages?category=<%= category._id %>">
                                                    <%= category.name %>
                                                </a>
                                            </li>
                                            <% } %>
                                                <% }) %>
                            </ul>
                        </li>
                        <!-- About Us -->
                        <li class="touria-dd">
                            <a href="/about">About Us</a>
                        </li>
                        <!-- Tours -->
                        <!-- Gallery -->
                        <li class="touria-dd">
                            <a href="/gallery">Gallery</a>
                        </li>
                        <!-- Blog -->
                        <li class="touria-dd">
                            <a href="/blogs">Blogs</a>

                        </li>
                        <!-- Contact -->
                        <li>
                            <a href="/contact" class="sub-menu-item">Contact Us</a>
                        </li>
                    </ul>

                    <!-- Header Navigation -->
                    <div class="header-navigation d-flex flex-wrap align-items-center gap-3 mt-4 mt-xl-0">
                        <!-- Search Button -->
                        <div class="header-search-btn" id="searchButton">
                            <button class="btn">
                                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 20 20"
                                    fill="none">
                                    <rect width="20" height="20" fill="#F7F7F7" />
                                    <path
                                        d="M14.0772 14.0987L16.6438 16.6654M15.833 9.58203C15.833 11.2396 15.1745 12.8293 14.0024 14.0014C12.8303 15.1736 11.2406 15.832 9.58301 15.832C7.9254 15.832 6.33569 15.1736 5.16359 14.0014C3.99149 12.8293 3.33301 11.2396 3.33301 9.58203C3.33301 7.92443 3.99149 6.33472 5.16359 5.16261C6.33569 3.99051 7.9254 3.33203 9.58301 3.33203C11.2406 3.33203 12.8303 3.99051 14.0024 5.16261C15.1745 6.33472 15.833 7.92443 15.833 9.58203Z"
                                        stroke="#161920" stroke-width="1.5" stroke-linecap="round"
                                        stroke-linejoin="round" />
                                </svg>
                            </button>
                        </div>
                        <!-- Get A Quote -->
                        <a class="btn btn-success" href="/contact">Enquire <i class="icon-arrow-right"></i></a>
                    </div>
                </div>
            </nav>
        </div>
    </header>

    <!-- Hero Section -->
    <section class="hero-section bg-dark">
        <!-- Cloud Image -->
        <div class="cloud-img" id="cloudImg"></div>
        <script>
            document.addEventListener('DOMContentLoaded', function () {
                const cloudImg = document.getElementById('cloudImg');
                let lastScrollY = window.scrollY;

                function updateCloudPosition() {
                    const scrollY = window.scrollY;
                    const maxMove = 60;
                    const maxScroll = 300;
                    let moveUp = Math.min(scrollY / maxScroll * maxMove, maxMove);
                    cloudImg.style.transform = `translateY(-${moveUp}px)`;
                }

                window.addEventListener('scroll', updateCloudPosition, { passive: true });
            });
        </script>

        <!-- Social Icons -->
        <div class="social-icons d-none d-sm-flex">
            <a href="https://www.facebook.com/joannaholidays1/" target="_blank"><i class="ti ti-brand-facebook"></i></a>
            <a href="https://wa.me/+918086602888" target="_blank"><i class="ti ti-brand-whatsapp"></i></a>
            <a href="tel:+918086602888" target="_blank"><i class="ti ti-phone"></i></a>
            <a href="https://www.instagram.com/joannaholidays/?hl=en" target="_blank"><i
                    class="ti ti-brand-instagram"></i></a>
        </div>

        <!-- Background Slider -->
        <div class="swiper background-swiper">
            <div class="swiper-wrapper h-100">
                <div class="swiper-slide h-100" style="background-image: url('/img/joanna/bg1.jpg')">
                </div>
                <div class="swiper-slide h-100" style="background-image: url('/img/joanna/bg2.jpg')">
                </div>
                <div class="swiper-slide h-100" style="background-image: url('/img/joanna/bg3.jpg')">
                </div>
                <div class="swiper-slide h-100" style="background-image: url('/img/joanna/bg4.jpg')">
                </div>
            </div>
        </div>

        <!-- Background Slider Navigation -->
        <div class="background-slider-nav d-none d-sm-flex">
            <div class="background-button-prev">
                <i class="icon-arrow-left"></i>
            </div>
            <div class="background-button-next">
                <i class="icon-arrow-right"></i>
            </div>
        </div>

        <div class="container">
            <div class="row">
                <div class="col-12 col-sm-9 offset-sm-1 col-xl-10 offset-xl-1 offset-xxl-0">
                    <!-- Hero Content -->
                    <div class="hero-content home-one">
                        <h3 class="ff-montez text-success wow fadeInUp" data-wow-delay="300ms"
                            data-wow-duration="1000ms">
                            Experience Unmatched Delight With Us.</h3>
                        <h2 class="text-white mb-4 wow fadeInUp" data-wow-delay="600ms" data-wow-duration="1000ms">Where
                            Exceptional <span style="color: #2b479e;">Memories</span>  <span style="color:#ed1c24 ;"> Begin!</span> </h2>
                        <p class="text-white wow fadeInUp" data-wow-delay="700ms" data-wow-duration="1000ms">Welcome to
                            Joanna Holidays. We are a professional and reliable tours company
                            that offers a wide range of printing services are many variations of passages.</p>
                        <div class="hero-search-form  my-5 wow fadeInUp" data-wow-delay="900ms"
                            data-wow-duration="1000ms">
                            <div class="card border-0 shadow-lg">
                                <div class="card-body p-4">
                                    <form class="row g-3" action="/packages" method="GET">
                                        <!-- Tour Type (Main Category) -->
                                        <div class="col-md-6 col-lg-3">
                                            <div class="form-floating position-relative">
                                                <div class="lottie-container"
                                                    style="position: absolute; right: 15px; top: 50%; transform: translateY(-50%); width: 40px; height: 40px; z-index: 5;">
                                                    <dotlottie-player
                                                        src="https://lottie.host/d659791b-0653-4e92-9eed-5c755143fa50/kT1uYDTObx.lottie"
                                                        background="transparent" speed="1"
                                                        style="width: 100%; height: 100%;" loop
                                                        autoplay></dotlottie-player>
                                                </div>
                                                <select name="category" id="search-category"
                                                    class="form-select form-select-lg ps-4">
                                                    <option value="">Select Type</option>
                                                    <% categories.forEach(category=> { %>
                                                        <option value="<%= category._id %>" <%=(query &&
                                                            query.category===category._id.toString()) ? 'selected' : ''
                                                            %>>
                                                            <%= category.name %>
                                                        </option>
                                                        <% }); %>
                                                </select>
                                                <label for="search-category"
                                                    class="d-flex align-items-center gap-2 ps-4">
                                                    <span>Tour Type</span>
                                                </label>
                                            </div>
                                        </div>

                                        <!-- Location (Subcategory) -->
                                        <div class="col-md-6 col-lg-3">
                                            <div class="form-floating position-relative">
                                                <div class="lottie-container"
                                                    style="position: absolute; right: 15px; top: 50%; transform: translateY(-50%); width: 40px; height: 40px; z-index: 5;">
                                                    <dotlottie-player
                                                        src="https://lottie.host/2edf5f89-183c-4e79-a5fc-0344b797ca35/GmaNHQgJE5.lottie"
                                                        background="transparent" speed="1"
                                                        style="width: 100%; height: 100%;" loop
                                                        autoplay></dotlottie-player>
                                                </div>
                                                <select name="subCategory" id="search-subcategory"
                                                    class="form-select form-select-lg ps-4" <%=!(query &&
                                                    query.category) ? 'disabled' : '' %>>
                                                    <option value="">Select Type First</option>
                                                    <% if (query && query.category) { %>
                                                        <% const selectedCat=categories.find(c=> c._id.toString() ===
                                                            query.category); %>
                                                            <% if (selectedCat && selectedCat.subCategories) { %>
                                                                <% selectedCat.subCategories.forEach(subCat=> { %>
                                                                    <option value="<%= subCat._id %>"
                                                                        <%=query.subCategory===subCat._id.toString()
                                                                        ? 'selected' : '' %>>
                                                                        <%= subCat.name %>
                                                                    </option>
                                                                    <% }); %>
                                                                        <% } %>
                                                                            <% } %>
                                                </select>
                                                <label for="search-subcategory"
                                                    class="d-flex align-items-center gap-2 ps-4">
                                                    <span>Select Location</span>
                                                </label>
                                            </div>
                                        </div>

                                        <!-- Date Range -->
                                        <div class="col-md-6 col-lg-3">
                                            <div class="form-floating position-relative">
                                                <input type="month" name="month" class="form-control form-control-lg ps-4"
                                                    id="search-month" placeholder="Select Month">
                                                <label for="search-month" class="d-flex align-items-center gap-2 ps-4">
                                                    <span>Select Month</span>
                                                </label>
                                            </div>
                                        </div>

                                        <!-- Submit Button -->
                                        <div class="col-md-6 col-lg-3 d-grid">
                                            <button type="submit" class="btn btn-primary btn-lg h-100">
                                                <i class="fas fa-search me-2"></i> Search Packages
                                            </button>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>

                        <script>
                            document.addEventListener('DOMContentLoaded', function () {
                                const categorySelect = document.getElementById('search-category');
                                const subcategorySelect = document.getElementById('search-subcategory');

                                // Create a mapping of categories to their subcategories
                                const categoryMap = {};
                          <% categories.forEach(category => { %>
                                    categoryMap['<%= category._id %>'] =[];
                            <% if (category.subCategories && category.subCategories.length > 0) { %>
                              <% category.subCategories.forEach(subCategory => { %>
                                <% if (subCategory.isActive) { %>
                                            categoryMap['<%= category._id %>'].push({
                                                id: '<%= subCategory._id %>',
                                                name: '<%= subCategory.name.replace(/\'/g, "\\'") %>'
                                  });
                                <% } %>
                              <% }); %>
                            <% } %>
                          <% }); %>

                                    // When category changes, update subcategories
                                    categorySelect.addEventListener('change', function () {
                                        const categoryId = this.value;
                                        subcategorySelect.innerHTML = '<option value="">Select Type First</option>';

                                        if (categoryId && categoryMap[categoryId] && categoryMap[categoryId].length > 0) {
                                            subcategorySelect.disabled = false;
                                            categoryMap[categoryId].forEach(subCat => {
                                                const option = new Option(subCat.name, subCat.id);
                                                subcategorySelect.add(option);
                                            });

                              // Check if there's a previously selected subcategory that belongs to this category
                              <% if (query && query.subCategory) { %>
                                const prevSubCat = '<%= query.subCategory %>';
                                                if (categoryMap[categoryId].some(sub => sub.id === prevSubCat)) {
                                                    subcategorySelect.value = prevSubCat;
                                                }
                              <% } %>
                            } else {
                                            subcategorySelect.disabled = true;
                                            subcategorySelect.innerHTML = '<option value="">No locations available</option>';
                                        }
                                    });

                                // Initialize with current category if present
                                if (categorySelect.value) {
                                    categorySelect.dispatchEvent(new Event('change'));
                                }

                                // Enable subcategory select if it has options on page load
                                if (subcategorySelect.options.length > 1) {
                                    subcategorySelect.disabled = false;
                                }

                                // Add hover effects to form controls
                                const formControls = document.querySelectorAll('.form-select, .form-control');
                                formControls.forEach(control => {
                                    control.addEventListener('mouseenter', () => {
                                        control.classList.add('shadow-sm');
                                        const lottie = control.parentElement.querySelector('dotlottie-player');
                                        if (lottie) lottie.setSpeed(1.5);
                                    });
                                    control.addEventListener('mouseleave', () => {
                                        control.classList.remove('shadow-sm');
                                        const lottie = control.parentElement.querySelector('dotlottie-player');
                                        if (lottie) lottie.setSpeed(1);
                                    });
                                    control.addEventListener('focus', () => {
                                        const lottie = control.parentElement.querySelector('dotlottie-player');
                                        if (lottie) lottie.setSpeed(2);
                                    });
                                    control.addEventListener('blur', () => {
                                        const lottie = control.parentElement.querySelector('dotlottie-player');
                                        if (lottie) lottie.setSpeed(1);
                                    });
                                });
                            });
                        </script>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Premier Destination Section -->
    <section class="premier-destination-section position-relative"
        style="position:relative; z-index:1; margin-top: 50px;">
        <!-- World Map Background -->
        <div style="position:absolute; inset:0; width:100%; height:100%; z-index:0; pointer-events:none;">
            <img src="/img/joanna/1.png" alt="World Map" style="width:100%; height:100%; object-fit:contain; ">
        </div>

        <div class="container position-relative" style="z-index:1;">
            <div class="row g-4 g-md-5 align-items-end justify-content-center" style="position:relative;">
                <div class="col-12 col-sm-8 col-md-7 col-lg-6" style="position:relative; margin-left:auto; margin-right:auto;">
                    <div class="section-heading d-flex align-items-center justify-content-center" style="width:100%; position:relative;">
                        <div>
                            <span class="sub-title text-info">Popular Tour Types</span>
                            <h2 class="mb-0 d-inline-block" style="vertical-align:middle;">International & Domestic</h2>
                        </div>
                        <!-- Plane animation for desktop -->
                        <div class="d-none d-sm-block"
                            style="position:absolute; left:100%; top:50%; transform:translateY(-50%); pointer-events:none; z-index:0;">
                            <dotlottie-wc
                                src="https://lottie.host/06723cc5-310a-4627-99fa-e04e00f7d2f2/JCn5xQAOoL.lottie"
                                style="width: 300px; height: 300px;" speed="1" autoplay loop>
                            </dotlottie-wc>
                        </div>
                        <!-- Plane animation for mobile -->
                        <div class="d-block d-sm-none"
                            style="position:static; margin-left:0; margin-top:10px; width:100%; text-align:left; z-index:0;">
                            <dotlottie-wc
                                src="https://lottie.host/06723cc5-310a-4627-99fa-e04e00f7d2f2/JCn5xQAOoL.lottie"
                                style="width: 100px; height: 100px;" speed="1" autoplay loop>
                            </dotlottie-wc>
                        </div>
                    </div>
                </div>
            </div>

            <div class="divider-sm"></div>

            <!-- Desktop grid: 4 per row, centered -->
            <div class="d-none d-sm-block">
                <div class="row g-4 justify-content-center">
                    <% categories.forEach(function(category, idx) { %>
                        <div class="col-6 col-md-4 col-lg-3 d-flex justify-content-center">
                            <div class="card h-100 shadow-sm border-0 position-relative premier-destination-card"
                                style="background-image: url('<%= category.imageUrl %>'); background-size:cover; background-position:center; min-height:220px;">
                                <div class="position-absolute top-0 start-0 w-100 h-100"
                                     style="background:rgba(0,0,0,0.35); border-radius:1rem 1rem 1rem 1rem;"></div>
                                <div class="card-body d-flex flex-column justify-content-end align-items-start position-relative" style="z-index:2; min-height:140px;">
                                    <div>
                                        <a href="/packages?category=<%= category._id %>" class="text-white fw-bold" style="font-size:1.25rem; text-shadow:0 2px 8px #000; text-transform: uppercase;">
                                            <%= category.name %>
                                        </a>
                                    </div>
                                    <% if (category.startingPrice) { %>
                                        <p class="mb-1 text-white" style="text-shadow:0 2px 8px #000;">
                                            Starting From <span class="text-info">$<%= category.startingPrice %></span>
                                        </p>
                                    <% } else { %>
                                        <p class="mb-1 text-white" style="text-shadow:0 2px 8px #000;">
                                            <% if (category.locationCount > 0) { %>
                                                <span class="text-info">
                                                    <%= category.locationCount %> Packages
                                                </span>
                                            <% } else { %>
                                                <span class="text-success">Currently Unavailable</span>
                                            <% } %>
                                        </p>
                                    <% } %>
                                    <a href="/packages?category=<%= category._id %>" class="btn btn-sm btn-light mt-2 px-3 shadow" style="font-weight:600;">
                                        <i class="ti ti-plane-tilt"></i> View Packages
                                    </a>
                                </div>
                            </div>
                        </div>
                    <% }); %>
                </div>
            </div>

            <!-- Mobile Swiper Slider: 1.2 cards per view, balance slider, centered -->
            <div class="d-block d-sm-none">
                <div class="swiper premier-destination-mobile-swiper">
                    <div class="swiper-wrapper">
                        <% categories.forEach(function(category, idx) { %>
                            <div class="swiper-slide d-flex justify-content-center" style="max-width: 85vw;">
                                <div class="card h-100 shadow-sm border-0 position-relative premier-destination-card"
                                    style="background-image: url('<%= category.imageUrl %>'); background-size:cover; background-position:center; min-height:200px;">
                                    <div class="position-absolute top-0 start-0 w-100 h-100"
                                         style="background:rgba(0,0,0,0.35); border-radius:1rem 1rem 1rem 1rem;"></div>
                                    <div class="card-body d-flex flex-column justify-content-end align-items-start position-relative" style="z-index:2; min-height:110px;">
                                        <div>
                                            <a href="/packages?category=<%= category._id %>" class="text-white fw-bold" style="font-size:1.1rem; text-shadow:0 2px 8px #000;">
                                                <%= category.name %>
                                            </a>
                                        </div>
                                        <% if (category.startingPrice) { %>
                                            <p class="mb-1 text-white" style="text-shadow:0 2px 8px #000;">
                                                Starting From <span class="text-info">$<%= category.startingPrice %></span>
                                            </p>
                                        <% } else { %>
                                            <p class="mb-1 text-white" style="text-shadow:0 2px 8px #000;">
                                                <% if (category.locationCount > 0) { %>
                                                    <span class="text-info">
                                                        <%= category.locationCount %> Packages Available
                                                    </span>
                                                <% } else { %>
                                                    <span class="text-danger">Currently Unavailable</span>
                                                <% } %>
                                            </p>
                                        <% } %>
                                        <a href="/packages?category=<%= category._id %>" class="btn btn-sm btn-light mt-2 px-3 shadow" style="font-weight:600;">
                                            <i class="ti ti-plane-tilt"></i> View Packages
                                        </a>
                                    </div>
                                </div>
                            </div>
                        <% }); %>
                    </div>
