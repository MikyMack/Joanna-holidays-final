        <script>
            window.allPackages = <%- JSON.stringify(packages) %>;
    
            function getCategoryId(pkg) {
                // Defensive: pkg.category may be null or undefined
                if (pkg.category && pkg.category._id) {
                    return pkg.category._id.toString();
                }
                return 'uncategorized';
            }
    
            function renderPackages(filteredPackages) {
                const container = document.getElementById('packages-container');
                container.innerHTML = '';
                filteredPackages.slice(0, 12).forEach((pkg, idx) => {
                    const discountHtml = pkg.discount > 0 ? `
    <span class="badge bg-danger position-absolute top-0 start-0 m-2 fs-6 animate__animated animate__bounceIn">
        <i class="ti ti-discount-2"></i> ${pkg.discount}% Off
    </span>
    ` : '';
    
                    const categoryHtml = pkg.categoryName ? `
    <span class="badge bg-primary position-absolute top-0 end-0 m-2 fs-6 animate__animated animate__fadeInRight">
        <i class="ti ti-star"></i> ${pkg.categoryName}
    </span>
    ` : '';
    
                    const priceHtml = pkg.packagePrice > 0
                        ? `₹${pkg.packagePrice.toLocaleString()}`
                        : 'Custom Pricing';
    
                    const cardHtml = `
    <div class="col-12 col-sm-6 col-lg-3 package-item animate__animated animate__fadeInUp" style="animation-delay: ${(idx * 0.07).toFixed(2)}s;" data-category="${getCategoryId(pkg)}">
        <div class="card h-100 shadow-sm border-0 position-relative">
            <div class="position-relative">
                <img src="${(pkg.images && pkg.images[0]) ? pkg.images[0] : '/images/listing/default.jpg'}"
                    class="card-img-top" alt="${pkg.title}"
                    style="height: 220px; object-fit: cover; border-top-left-radius: 1.5rem; border-top-right-radius: 1.5rem;">
                ${discountHtml}
                ${categoryHtml}
            </div>
            <div class="card-body d-flex flex-column">
                <div class="mb-2 text-muted small">
                    <i class="ti ti-map-pin text-danger"></i>
                    ${pkg.destination}
                </div>
                <h5 class="card-title mb-1">
                    <a href="/package/${pkg._id}" class="stretched-link text-decoration-none text-dark fw-semibold">
                        ${pkg.title}
                    </a>
                </h5>
                <div class="mb-2 text-secondary small">
                    <i class="mdi mdi-clock-outline text-primary"></i>
                    ${pkg.duration}
                </div>
                <p class="card-text text-truncate mb-2" title="${pkg.shortDescription}">
                    ${pkg.shortDescription}
                </p>
                <div class="mt-auto d-flex justify-content-between align-items-center pt-2 border-top">
                    <div>
                        ${pkg.packagePrice > 0
                            ? `<span class="fw-bold text-success fs-5">₹${pkg.packagePrice.toLocaleString()}</span>`
                            : `<span class="fw-bold text-warning fs-6">Custom Pricing</span>`
                        }
                    </div>
                    <a href="/package/${pkg._id}" class="btn btn-outline-danger btn-sm animate__animated animate__pulse animate__infinite">
                        Explore <i class="mdi mdi-arrow-right"></i>
                    </a>
                </div>
            </div>
        </div>
    </div>
    `;
                    container.insertAdjacentHTML('beforeend', cardHtml);
                });
    
                if (window.feather) feather.replace();
            }
    
            function filterPackages(categoryId) {
                // Update active button styling
                document.querySelectorAll('.category-btn').forEach(btn => {
                    if (btn.dataset.category === categoryId) {
                        btn.classList.add('btn-danger', 'active');
                        btn.classList.remove('btn-outline-secondary');
                    } else {
                        btn.classList.remove('btn-danger', 'active');
                        btn.classList.add('btn-outline-secondary');
                    }
                });
    
                // Filter packages and show only 12
                let filtered = [];
                if (categoryId === 'all') {
                    filtered = window.allPackages;
                } else {
                    filtered = window.allPackages.filter(pkg =>
                        getCategoryId(pkg) === categoryId
                    );
                }
                renderPackages(filtered);
    
                // Show/hide no packages message
                const noPackagesMessage = document.getElementById('no-packages-message');
                const packagesContainer = document.getElementById('packages-container');
    
                if (filtered.length === 0) {
                    noPackagesMessage.classList.remove('d-none');
                    packagesContainer.classList.add('d-none');
                } else {
                    noPackagesMessage.classList.add('d-none');
                    packagesContainer.classList.remove('d-none');
                }
            }
    
            document.addEventListener('DOMContentLoaded', function () {
                // Attach event listeners to category buttons
                document.querySelectorAll('.category-btn').forEach(btn => {
                    btn.addEventListener('click', function (e) {
                        e.preventDefault();
                        filterPackages(this.dataset.category);
                    });
                });
                // Initial render
                filterPackages('all');
                if (window.feather) feather.replace();
            });
        </script>
